<%- include("partials/header") -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="">Title</label><br>
        <input type="text" class="form-control" name="postTitle" placeholder="Write here .." autocomplete="off">
        
    </div>
   <div class="form-group">
    <label for="">Post</label>
    <textarea type="text" class="form-control" rows="5" cols="30" name="postBody"></textarea>
   </div>
<button name="button" type="submit" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer") -%>